<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>LogLet</servlet-name>
        <servlet-class>com.servlet.LogLet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegLet</servlet-name>
        <servlet-class>com.servlet.RegLet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonalInfoLet</servlet-name>
        <servlet-class>com.servlet.PersonalInfoLet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditInfoLet</servlet-name>
        <servlet-class>com.servlet.EditInfoLet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestLet</servlet-name>
        <servlet-class>com.servlet.TestLet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LogLet</servlet-name>
        <url-pattern>/LogLet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegLet</servlet-name>
        <url-pattern>/RegLet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PersonalInfoLet</servlet-name>
        <url-pattern>/PersonalInfoLet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditInfoLet</servlet-name>
        <url-pattern>/EditInfoLet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestLet</servlet-name>
        <url-pattern>/TestLet</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>